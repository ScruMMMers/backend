{
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "database.dbname": "internship",
    "database.history.kafka.bootstrap.servers": "internship_broker:9092",
    "database.history.kafka.topic": "notification",
    "database.hostname": "db",
    "database.password": "password",
    "database.port": "5432",
    "database.server.name": "postgres",
    "database.user": "postgres",
    "name": "notification-db-connector",
    "plugin.name": "pgoutput",
    "table.include.list": "public.notification",
    "topic.creation.default.cleanup.policy": "delete",
    "topic.creation.default.partitions": "3",
    "topic.creation.default.replication.factor": "1",
    "topic.creation.default.retention.ms": "604800000",
    "topic.creation.enable": "true",
    "transforms.AddPrefix.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.AddPrefix.regex": "postgres.public.notification",
    "transforms.AddPrefix.replacement": "raw.notification",
    "topic.prefix": "raw",
    "message.key.columns": "public.notification:*",
    "key.converter.schemas.enable": "false",
    "value.converter.schemas.enable": "false",
    "decimal.handling.mode": "string",
    "slot.name" : "notification_slot"
  },
  "name": "notification-db-connector"
}